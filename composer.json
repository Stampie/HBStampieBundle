{
    "name": "henrikbjorn/stampie-bundle",
    "description": "This bundle provides integration for Stampie Email Library",
    "keywords": ["symfony", "stampie", "email"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name" : "Henrik BjÃ¸rnskov",
        "email" : "henrik@bjrnskov.dk"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.3.3",
        "stampie/stampie": "^1.0@beta",
        "symfony/console": "^2.7 || ^3.0",
        "symfony/framework-bundle": "^2.7 || ^3.0"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.4",
        "php-http/curl-client": "^1.7",
        "stampie/extra": "^1.0@beta",
        "symfony/phpunit-bridge": "^3.2"
